{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "id",
      "step"
    ],
    "properties": {
      "id": {
        "type": "string",
        "pattern": "^WF-\\d{3}$"
      },
      "step": {
        "type": "string"
      }
    }
  }
}